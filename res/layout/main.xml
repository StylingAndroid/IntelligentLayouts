			<ImageView android:src="@drawable/icon"
				android:layout_width="wrap_content" android:layout_height="wrap_content" />
			<TextView android:text="Nine" android:layout_width="wrap_content"
				android:layout_height="wrap_content" />
		</LinearLayout>
	</LinearLayout>
</LinearLayout>
